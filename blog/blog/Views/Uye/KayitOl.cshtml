
@{
    Layout = null;
    if (Request.Cookies["User"] != null)
    {
        Response.Redirect("/Home/");
    }
}


<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login V13</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/Content/SignUp_v13/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Content/SignUp_v13/css/main.css">
    <!--===============================================================================================-->
</head>
<body style="background-color: #999999;">

    <div class="limiter">
        <div class="container-login100">
            <div class="login100-more" style="background-image: url('../../Content/SignUp_v13/images/bg-01.jpg');"></div>

            <div class="wrap-login100 p-l-50 p-r-50 p-t-72 p-b-50">
                <form class="login100-form validate-form" action="/Uye/KayitOl" method="post">
                    <span class="login100-form-title p-b-59">
                        Kayıt Ol
                    </span>

                    <div class="wrap-input100 validate-input" data-validate="Ad gerekli">
                        <span class="label-input100">Ad</span>
                        <input class="input100" type="text" name="Ad" placeholder="Ad...">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Soyad gerekli">
                        <span class="label-input100">Soyad</span>
                        <input class="input100" type="text" name="Soyad" placeholder="Soyad...">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Doğum tarihi gerekli">
                        <span class="label-input100">Doğum Tarihi</span>
                        <input class="input100" type="date" name="DogumTarihi">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Doğum tarihi gerekli">
                        <span class="label-input100">Doğum Tarihi</span>
                        <select name="Cinsiyet" class="input100">
                            <option value="erkek">Erkek</option>
                            <option value="kadın">Kadın</option>
                        </select>
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Kullanıcı adı gerekli">
                        <span class="label-input100">Kullanıcı Adı</span>
                        <input class="input100" type="text" name="KullaniciAdi" placeholder="Kullanıcı adı...">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Uygun format: ex@abc.xyz">
                        <span class="label-input100">Email</span>
                        <input class="input100" type="text" name="Email" placeholder="Email...">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Parola gerekli">
                        <span class="label-input100">Parola</span>
                        <input class="input100" type="text" name="Parola" placeholder="*************">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="Parola gerekli">
                        <span class="label-input100">Parola(Tekrar)</span>
                        <input class="input100" type="text" name="ParolaTekrar" placeholder="*************">
                        <span class="focus-input100"></span>
                    </div>

                    @*<div class="flex-m w-full p-b-33">
                            <div class="contact100-form-checkbox">
                                <input class="input-checkbox100" id="ckb1" type="checkbox" name="remember-me">
                                <label class="label-checkbox100" for="ckb1">
                                    <span class="txt1">
                                        I agree to the
                                        <a href="~/Content/SignUp_v13/#" class="txt2 hov1">
                                            Terms of User
                                        </a>
                                    </span>
                                </label>
                            </div>
                        </div>*@

                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button type="submit" class="login100-form-btn">
                                Kayıt Ol
                            </button>
                        </div>

                        <a href="/Uye/Giris" class="dis-block txt3 hov1 p-r-30 p-t-10 p-b-10 p-l-30">
                            Giriş Yap
                            <i class="fa fa-long-arrow-right m-l-5"></i>
                        </a>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <!--===============================================================================================-->
    <script src="~/Content/SignUp_v13/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Content/SignUp_v13/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Content/SignUp_v13/vendor/bootstrap/js/popper.js"></script>
    <script src="~/Content/SignUp_v13/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Content/SignUp_v13/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Content/SignUp_v13/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/Content/SignUp_v13/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/Content/SignUp_v13/vendor/countdowntime/countdowntime.js"></script>
    <!--SweetAlertJS-->
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/Content/SignUp_v13/js/main.js"></script>

    <script type="text/javascript">
        $(function () {
            @Html.Raw(ViewBag.Hata)
        });
    </script>

</body>
</html>